<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <proxies>
        <proxy>
            <id>fidu-prox</id>
            <active>true</active>
            <protocol>http</protocol>
            <host>proxy01-ka.noc.fiducia.de</host>
            <port>8080</port>
            <nonProxyHosts>localhost|127.0.0.0/24|10.0.0.0/24|192.168.0.0/16|.fiducia.de|.fiduciagad.de|.gad.de
            </nonProxyHosts>
        </proxy>
        <proxy>
            <id>fidu-https-prox</id>
            <active>true</active>
            <protocol>https</protocol>
            <host>proxy01-ka.noc.fiducia.de</host>
            <port>8080</port>
            <nonProxyHosts>localhost|127.0.0.0/24|10.0.0.0/24|192.168.0.0/16|.fiducia.de|.fiduciagad.de|.gad.de
            </nonProxyHosts>
        </proxy>
    </proxies>
    <servers>
    </servers>
    <mirrors>
    </mirrors>
    <profiles>
        <profile>
            <id>myprofile</id>
            <repositories>
                <repository>
                    <id>mvn central</id>
                    <name>Maven apache org</name>
                    <url>http://repo.maven.apache.org/maven2</url>
                </repository>
                <repository>
                    <id>mvnrepository</id>
                    <name>Maven central</name>
                    <url>http://central.maven.org/maven2/</url>
                </repository>
            </repositories>
        </profile>
    </profiles>

    <activeProfiles>
        <activeProfile>myprofile</activeProfile>
    </activeProfiles>
</settings>
